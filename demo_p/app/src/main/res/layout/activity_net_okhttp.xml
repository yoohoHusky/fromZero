<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yooho.husky.myapplication.MainActivity">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <Button
                android:id="@+id/btn_okhttp_async_get"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="async_get"/>
            <Button
                android:id="@+id/btn_okhttp_sync_get"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="sync_get"/>
            <Button
                android:id="@+id/btn_okhttp_async_post"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="async_post"/>
        </LinearLayout>
</ScrollView>
